{% extends 'base.html.twig' %}
 {% block stylesheets %}
 <link rel="stylesheet" href="{{ 'css/search.css' }}">
 {% endblock %}
{% block title %}Team{% endblock %}

{% block body %}
<main class="wrapper">
<section class="search_page">
<a class="nav_link mr" href="{{ path('app_team')}}">Retour</a>
<h1>Liste des sujets chez {{ app.user.team.nom }}</h1>
<ul>
{% for sujet in app.user.team.sujets %}
<li><a class="nav_link mr" href="{{ path('app_forum_id', { 'id': sujet.id } )}}">{{ sujet.titre }}</a></li>
{% endfor %}
</ul>
</section>
</main>
{% endblock %}